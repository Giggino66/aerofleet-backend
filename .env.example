# AeroFleet Manager - Environment Variables

# Database (Render will provide this automatically)
DATABASE_URL=postgresql://user:password@localhost:5432/aerofleet

# Security (generate a secure random string for production)
SECRET_KEY=your-secret-key-here-change-this

# CORS (add your frontend domains)
CORS_ORIGINS=http://localhost:3000,https://yourdomain.com

# Debug (set to False in production)
DEBUG=False
